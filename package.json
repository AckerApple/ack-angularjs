{
  "name": "ack-angularjs",
  "version": "1.7.4",
  "description": "Universal Angular base code suited for any web app",
  "main": "ack-angular.js",
  "private": true,
  "scripts": {
    "build:localforage":"node scripts/build-localforage.js",
    "build:js:ack-angular": "webpack src/ack-angular.js www/ack-angular.js --optimize-minimize --production",
    "build:js:ack-state": "webpack src/ack-state.module.js www/ack-state.js --optimize-minimize --production",
    "build:js": "npm run build:js:ack-angular && npm run build:js:ack-state",
    "watch:js": "npm run watch:js:ack-angular & npm run watch:js:ack-state",
    "watch:js:ack-angular": "webpack src/ack-angular.js www/ack-angular.js --watch",
    "watch:js:ack-state": "webpack src/ack-state.module.js www/ack-state.js --watch",
    "test": "ionic serve",
    "build:ng-fx": "node scripts/build-ng-fx",
    "build": "npm run build:ng-fx && npm run build:pug && npm run build:examples && npm run build:test && npm run build:css",
    "build:css": "npm run build:css:ack-css-boot && npm run build:css:csshake && npm run build:css:www && npm run build:css:dist",
    "build:css:csshake": "ack-sass scripts/scss/csshake/csshake.scss scss/csshake.scss --production",
    "build:css:ack-css-boot": "ack-sass scripts/scss/ack-css-boot/ack-css-boot.scss scss/ack-css-boot.scss --production",
    "build:css:www": "ack-sass scss/styles.scss www/styles.css --production",
    "build:css:dist": "ack-sass scss/styles.scss dist/styles.css --production",
    "build:test:index": "pug ./test-src/index.pug --out ./www/",
    "build:examples": "npm run build:js && npm run build:examples:pug",
    "build:examples:pug": "ack-pug-bundler ./test-src/examples.pug ./www/examples.html --oneHtmlFile",
    "build:pug": "ack-pug-bundler src",
    "build:test": "npm run build:test:index && npm run build:test:js",
    "watch:test": "npm run watch:test:index & npm run watch:test:js",
    "watch:test:index": "watch 'npm run build:test:index' src",
    "build:test:js": "webpack test-src/test.js www/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AckerApple/ack-angularjs.git"
  },
  "keywords": [
    "acker",
    "apple",
    "directives"
  ],
  "author": "Acker Apple",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AckerApple/ack-angularjs/issues"
  },
  "homepage": "https://github.com/AckerApple/ack-angularjs",
  "devDependencies": {
    "ack-css-boot": "^1.2.0",
    "ack-pug-bundler": "^1.1.2",
    "ack-sass": "^1.0.8",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "csshake": "^1.5.0",
    "ng-fx": "^2.0.4",
    "node-sass": "^3.7.0",
    "pug": "^2.0.0-beta4",
    "watch": "^0.19.1",
    "webpack": "^1.14.0",
    "angular": "^1.6.0"
  },
  "dependencies": {
    "ack-x": "^1.2.20",
    "angular-activity-monitor": "^1.1.0",
    "angular-animate": "^1.6.0",
    "angular-mocks": "github:angular/bower-angular-mocks",
    "angular-ui-router": "^0.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "json-loader": "^0.5.4",
    "localforage": "^1.4.3"
  }
}
